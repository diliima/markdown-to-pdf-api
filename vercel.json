{
  "version": 2,
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/verificar",
      "dest": "/api/index"
    },
    {
      "src": "/converter-markdown-pdf-base64",
      "dest": "/api/index"
    },
    {
      "src": "/converter-markdown-pdf",
      "dest": "/api/index"
    },
    {
      "src": "/",
      "dest": "/api/index"
    }
  ]
}
